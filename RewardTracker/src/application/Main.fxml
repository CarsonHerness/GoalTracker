<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label fx:id="points" layoutX="442.0" layoutY="14.0" text="Points">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="redeemPoints" layoutX="181.0" layoutY="164.0" mnemonicParsing="false" onAction="#redeemPoints" text="Redeem Points">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="enterPoints" layoutX="193.0" layoutY="104.0" mnemonicParsing="false" onAction="#enterPoints" text="Enter Points">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="addTaskOrReward" layoutX="164.0" layoutY="225.0" mnemonicParsing="false" onAction="#addTaskOrReward" text="Add Task or Reward">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="116.0" layoutY="76.0" text="Choose one of the options below.">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="19.0" text="Welcome to Reward Tracker!">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
