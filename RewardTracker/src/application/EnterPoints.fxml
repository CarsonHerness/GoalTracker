<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.EnterPointsController">
   <children>
      <TextField fx:id="submitPointsMinutes" layoutX="176.0" layoutY="181.0" promptText="Enter number of minutes" />
      <Label layoutX="187.0" layoutY="57.0" text="Enter Points">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="submitPoints" layoutX="224.0" layoutY="214.0" mnemonicParsing="false" onAction="#submitPoints" text="Submit" />
      <ComboBox fx:id="submitPointsTaskNameList" layoutX="278.0" layoutY="137.0" prefWidth="150.0" promptText="Choose Task" />
      <ComboBox fx:id="submitPointsTaskTypeList" layoutX="63.0" layoutY="137.0" onAction="#chooseTaskType" prefWidth="150.0" promptText="Choose Task Type" />
      <Label fx:id="submitPointsErrorMessage" alignment="CENTER" layoutX="10.0" layoutY="263.0" prefHeight="20.0" prefWidth="481.0" textAlignment="CENTER" textFill="#f51414">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="enterPointsMenuButton" layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#backToMainMenu" text="Back to Main Menu" />
   </children>
</AnchorPane>
